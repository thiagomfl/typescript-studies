{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "accessor-pairs": "off",
      "no-use-before-define":"off",
      "no-useless-constructor": "off",
      "space-before-function-paren": "off"
    }
}
